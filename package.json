{
  "displayName": "Homemade MQTT Smart Lock Plugin",
  "name": "@nzws/homebridge-mqtt-smartlock",
  "version": "0.1.1",
  "description": "Homebridge Plugin for homemade smart lock created with mqtt",
  "license": "Apache-2.0",
  "publishConfig":{
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nzws/homebridge-mqtt-smartlock.git"
  },
  "bugs": {
    "url": "https://github.com/nzws/homebridge-mqtt-smartlock/issues"
  },
  "engines": {
    "node": ">=10.17.0",
    "homebridge": ">=1.3.0"
  },
  "main": "dist/index.js",
